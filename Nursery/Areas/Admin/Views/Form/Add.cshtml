                        <div class="fg-col">
                            <label tool-label class="fg-label">ماه</label>
                            <input type="month" class="entry" autocomplete="off"/>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </aside>
    <div class="toolbox-placeholder" role="none"></div>
    <div class="prototype form">
        <div class="form-header col">
            <div class="fg-col uk-margin-small-bottom">
                <label class="fg-label">نام فرم</label>
                <input type="text" id="txtFormName" placeholder="فرم جدید..." class="entry" value="" required />
            </div>
            <div class="fg-col">
                <label class="fg-label">توضیحات</label>
                <textarea class="entry" id="txtDecsription" placeholder="..." required></textarea>
            </div>


            <div class="fg-col ">
                <label class="fg-label" for="name">اولویت فرم</label>
                <select class="uk-select" id="txtPriority">
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
                </select>
                <span class="text-danger" ></span>
            </div>

        </div>
        <div class="form-body gap-3"
             id="prototype"
             draggable="true"
             uk-sortable="group:form;handle: .tool-actions; cls-item:grabbable; cls-custom: tool-grabbed">
        </div>
        <div class="form-footer">
            <div class="actions">
                @if (ViewBag.pageId > 0)
                {
                    <button id="btn-finish" goto="/Admin/Page?id=@ViewBag.pageId&name=@ViewBag.name&addForm=true" class="btn btn-primary">
                        <span>تایید</span>
                    </button>
                }
                else
                {
                    <button id="btn-finish" goto="/Admin/Form/List?addForm=true" class="btn btn-primary">
                        <span>تایید</span>
                    </button>
                }

            </div>
        </div>
    </div>
</div>

